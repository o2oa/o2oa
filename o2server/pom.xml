										<directory>target</directory>
										<includes>
											<include>${project.artifactId}.war</include>
										</includes>
									</resource>
								</resources>
							</configuration>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>org.apache.openjpa</groupId>
