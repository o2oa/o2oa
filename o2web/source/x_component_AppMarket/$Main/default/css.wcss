        "color": "#666666",
        "font-size": "14px",
        "padding-left": "6px"
    },
    "moduleSetupListAreaTitleNode": {
        "height": "30px",
        "line-height": "30px",
        "background-color": "#dddddd",
        "color": "#333333",
        "padding-left": "10px",
        "font-weight": "bold",
        "font-size": "14px"
    },
    "moduleSetupListAreaContentNode": {
        "min-height": "30px",
        "background-color": "#ffffff"
    },
    "moduleSetupListContentNode": {
        "height": "36px",
        "line-height": "36px",
        "margin": "0px 10px",
        "font-size": "14px",
        "color": "#666666",
        "border-bottom": "1px solid #eeeeee"
    },
    moduleSetupListIconNode: {
        "height": "36px",
        "float": "left",
        "width": "36px"
    },
    moduleSetupListNameNode: {
        "height": "36px",
        "margin-right": "160px",
        "margin-left": "36px"
    },
    moduleSetupListActionNode: {
        "height": "36px",
        "float": "right",
        "width": "60px"
    },
    moduleSetupListInforNode: {
        "height": "36px",
        "float": "right",
        "width": "100px"
    },
    "moduleSetupListActionSelectNode": {
        "border": "1px solid #333333"
    }

}